{
  "name": "revane",
  "version": "1.0.2",
  "description": "Framework for building web applications without boilerplate code.",
  "main": "bin/src/revane/Revane.js",
  "scripts": {
    "unit": "tap bin/test/**/*Test.js",
    "test": "npm run lint && npm run compile && npm run unit",
    "lint": "tslint --project tsconfig.json --config tslint.yaml",
    "coveralls": "npm run compile && npm run unit --  --cov",
    "coverage": "npm run coveralls && tap --coverage-report=lcov",
    "compile": "tsc -d",
    "prepublishOnly": "npm run compile -s"
  },
  "engines": {
    "node": ">= 8.0.0"
  },
  "author": "Denis FÃ¤cke",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SerayaEryn/revane.git"
  },
  "dependencies": {
    "revane-fastify": "^2.0.2",
    "revane-ioc": "^1.0.2",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@types/node": "^11.11.0",
    "coveralls": "^3.0.1",
    "request": "^2.88.0",
    "tap": "^12.6.0",
    "tslint": "^5.10.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^2.9.1"
  },
  "keywords": [
    "inversion of control",
